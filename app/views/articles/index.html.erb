<%= render 'header', article: @article %>
<div class="container row justify-content-center">
  <article class="list-unstyled col-8">
  <% @articles.each do |article| %>
    <h2><%= link_to article.title, article_path(article) %></h2>
    <p><%= article.content %></p>
  <% end %>
  </article>
</div>
